class Solution(object):
    def minimumPairRemoval(self, nums):
        n = len(nums)
        dp = [1] * n 

        for i in range(n):
            for j in range(i):
                if nums[j] <= nums[i]:
                    dp[i] = max(dp[i], dp[j] + 1)

        lnds = max(dp)
        return n - lnds
